{
  "name": "wiki-github-proxy",
  "version": "1.0.0",
  "description": "Simple GitHub API proxy server",
  "main": "server.js",
  "scripts": {
    "start": "pm2 start ecosystem.config.js --env development",
    "start:prod": "pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop wiki-proxy",
